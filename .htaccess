<IfModule mod_rewrite.c>

RewriteEngine On

RewriteBase /
RewriteRule ^index\.php$ - [L]

RewriteRule ([^\?]+)\.(jpg|png|jpeg)\&(.+)$ /img.php?src=$1.$2&$3&trm=thumb [L,NC]

RewriteRule ^theme/(.*)		/wp-content/themes/go3/$1 [QSA,L]

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.php [L]

</IfModule>

